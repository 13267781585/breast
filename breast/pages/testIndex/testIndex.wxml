<block wx:for="{{testList}}" wx:for-index="idx">
	<view class="cu-card article " bindtap="chageToOp" data-index="{{idx}}">
		<view class="cu-item shadow">
			<view class="title">
				<view class="text-cut">{{item.tTitle}}</view>
			</view>
			<view class="content">
				<image src="{{item.imgUrl}}" mode="aspectFill"></image>
				<view class="desc">
					<view class="text-content">{{item.content}}</view>
					<view>
						<view class="cu-tag bg-red light sm round">母乳喂养</view>
						<!-- <view class="cu-tag bg-green light sm round">史诗</view> -->
					</view>
				</view>
			</view>
		</view>
	</view>
</block>


